{
  "name": "electron-nuxt3",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.6.0+sha512.38dc6fba8dba35b39340b9700112c2fe1e12f10b17134715a4aa98ccf7bb035e76fd981cf0bb384dfa98f8d6af5481c2bef2f4266a24bfa20c34eb7147ce0b5e",
  "description": "Nuxt3 + Electron & More",
  "author": "EternalC0DER",
  "email": "s.eternalcoder@gmail.com",
  "main": ".output/electron/background.js",
  "scripts": {
    "dev": "nuxt dev -o",
    "build": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && electron-builder install-app-deps",
    "transpile-electron": "tsc -p ./electron --outDir .output/electron",
    "dev:electron": "cross-env NODE_ENV=development concurrently --kill-others \"nuxt dev\" \"tsc-watch -p ./electron --outDir .output/electron --onSuccess 'electron ./.output/electron/background.js'\"",
    "dev:electron:win": "cross-env NODE_ENV=development concurrently --kill-others \"nuxt dev\" \"tsc-watch -p ./electron --outDir .output/electron --onSuccess run.electron\"",
    "build:electron": "pnpm build && pnpm transpile-electron && node build.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@pinia/nuxt": "^0.5.4",
    "@unovis/ts": "1.4.3-beta.0",
    "@unovis/vue": "1.4.3-beta.0",
    "@vee-validate/zod": "^4.13.2",
    "@vueuse/core": "^11.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "electron-log": "^5.1.7",
    "electron-updater": "^6.2.1",
    "embla-carousel-vue": "^8.2.0",
    "express": "^4.19.2",
    "lucide-vue-next": "^0.436.0",
    "radix-vue": "^1.9.5",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "v-calendar": "^3.1.2",
    "vaul-vue": "^0.2.0",
    "vee-validate": "^4.13.2",
    "vue-sonner": "^1.1.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.3",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/tailwindcss": "^6.12.1",
    "@types/express": "^4.17.21",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^32.0.1",
    "electron-builder": "^24.13.3",
    "eslint": "^9.9.1",
    "nuxt": "^3.13.0",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.5.4",
    "wait-on": "^8.0.0"
  }
}
